<?xml version="1.0"?>
<info>
	<id>sumupbot</id>
	<name>Sumupbot</name>
	<summary>Nextcloud Sumup Bot Example</summary>
	<description>
	<![CDATA[Example of the Nextcloud Sumup Bot written in python]]>
	</description>
	<version>1.0.0</version>
	<licence>MIT</licence>
	<author mail="florian.klinger@nextcloud.com" homepage="https://github.com/nc-fkl">Florian Klinger</author>
	<namespace>SumupBotExample</namespace>
	<category>tools</category>
	<website>https://github.com/cloud-py-api/nc_py_api</website>
	<bugs>https://github.com/cloud-py-api/nc_py_api/issues</bugs>
	<repository type="git">https://github.com/cloud-py-api/nc_py_api</repository>
	<dependencies>
		<nextcloud min-version="27" max-version="29"/>
	</dependencies>
	<external-app>
		<docker-install>
			<registry>ghcr.io</registry>
			<image>cloud-py-api/sumupbot</image>
			<image-tag>latest</image-tag>
		</docker-install>
		<scopes>
			<required>
				<value>TALK</value>
				<value>TALK_BOT</value>
				<value>AI_PROVIDERS</value>
				<value>NOTIFICATIONS</value>
			</required>
			<optional>
			</optional>
		</scopes>
		<protocol>http</protocol>
		<system>false</system>
	</external-app>
</info>
