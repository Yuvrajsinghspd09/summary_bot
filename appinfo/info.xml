<?xml version="1.0"?>
<info>
	<id>summarai</id>
	<name>SummarAI</name>
	<summary>Nextcloud chat summarize bot example</summary>
	<description>
	<![CDATA[Example of the Nextcloud chat summarize bot written in python]]>
	</description>
	<version>1.0.0</version>
	<licence>MIT</licence>
	<author mail="florian.klinger@nextcloud.com" homepage="https://github.com/nc-fkl">Florian Klinger</author>
	<namespace>SummarAI</namespace>
	<category>tools</category>
	<website>https://github.com/cloud-py-api/nc_py_api</website>
	<bugs>https://github.com/cloud-py-api/nc_py_api/issues</bugs>
	<repository type="git">https://github.com/cloud-py-api/nc_py_api</repository>
	<dependencies>
		<nextcloud min-version="27" max-version="29"/>
	</dependencies>
	<external-app>
		<docker-install>
			<registry>ghcr.io</registry>
			<image>cloud-py-api/summarai</image>
			<image-tag>latest</image-tag>
		</docker-install>
		<scopes>
			<value>TALK</value>
			<value>TALK_BOT</value>
			<value>AI_PROVIDERS</value>
			<value>NOTIFICATIONS</value>
			<value>USER_INFO</value>
			<value>SYSTEM</value>
		</scopes>
		<system>true</system>
	</external-app>
</info>
